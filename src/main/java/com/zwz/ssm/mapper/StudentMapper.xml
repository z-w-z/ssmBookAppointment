<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zwz.ssm.mapper.StudentMapperMapper">
  <select id="queryStudent" resultType="com.zwz.ssm.po.Student">
    SELECT
      student_id
      password
    FROM
      student
    WHERE
    student_id=#{studentzId}
    and password = #{password}
  </select>
</mapper>